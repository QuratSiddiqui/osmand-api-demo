				android:drawableLeft="@drawable/ic_action_polygom_dark"/>

			<View android:layout_width="match_parent" style="@style/divider_style"/>

			<android.support.v7.widget.AppCompatButton
				android:id="@+id/aidlSetMapLocationButton"

				android:layout_height="wrap_content"
				android:text="@string/update_map_marker"
				android:drawableLeft="@drawable/ic_action_flag_dark"/>

			<android.support.v7.widget.AppCompatButton
				android:id="@+id/aidlRemoveMapMarkerButton"
				style="@style/row_item_style"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:text="@string/remove_map_marker"
				android:drawableLeft="@drawable/ic_action_flag_dark"/>

			<View android:layout_width="match_parent" style="@style/divider_style"/>

			<android.support.v7.widget.AppCompatButton
				android:id="@+id/aidlAddFirstWidgetButton"
				style="@style/row_item_style"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:text="@string/add_map_widget_1"
				android:drawableLeft="@drawable/ic_action_gabout_dark"/>

			<android.support.v7.widget.AppCompatButton
				android:id="@+id/aidlAddSecondWidgetButton"
				style="@style/row_item_style"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:text="@string/add_map_widget_2"
				android:drawableLeft="@drawable/ic_action_gabout_dark"/>

			<android.support.v7.widget.AppCompatButton
				android:id="@+id/aidlModifyFirstWidgetButton"
				style="@style/row_item_style"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:text="@string/modify_map_widget_1"
				android:drawableLeft="@drawable/ic_action_gabout_dark"/>

			<android.support.v7.widget.AppCompatButton
				android:id="@+id/aidlModifySecondWidgetButton"
				style="@style/row_item_style"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:text="@string/modify_map_widget_2"
